        {% widget_attribute "facebook" is_json=True %}{"custom_link_format": "", "enabled": true, "network": "facebook", "img_src": "https://www.captio.net/hubfs/_assets/newsletter/blog/ico-facebook.jpg"}{% end_widget_attribute %}
        {% widget_attribute "email" is_json=True %}{"custom_link_format": "", "enabled": true, "network": "email", "img_src": "https://www.captio.net/hubfs/_assets/newsletter/blog/ico-email.jpg"}{% end_widget_attribute %}
