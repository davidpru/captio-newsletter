<mjml>

  <mj-head>

    <mj-attributes>
      <mj-all font-family="'Helvetica Neue', Helvetica, Arial, sans-serif"></mj-all>
      <mj-text 
        font-weight="400" 
        font-size="16px"
        line-height="24px"
        color="#5B636D" 
        font-family="'Helvetica Neue', Helvetica, Arial, sans-serif">
      </mj-text>
      
      <mj-class 
        name="h2" 
        font-weight="bold" 
        font-size="20px" 
        line-height="30px" 
        color="#323C49"
        margin-top="0"
        />

      <mj-class 
        name="h3" 
        font-weight="bold" 
        font-size="23px" 
        line-height="36px" 
        color="#fff"
        margin-top="0"
        />

      <mj-class 
        name="bg-gris" 
        background-color="#F0F0F0"
        overflow="hidden"
        />

      <mj-class 
        name="bg-amarillo" 
        background-color="#F9BA14"
        border-radius="16px"
        color="#fff"
        />

      <mj-class 
        name="bg-azul" 
        background-color="#2798C0"
        border-radius="16px"
        color="#fff"
        />

      <mj-class 
        name="bg-dark" 
        background-color="#323C49"
        color="#fff"
        border-radius="16px"
        />

      <mj-class 
        name="bg-blanco" 
        background-color="#fff"
        />

      <mj-class 
        name="radius-top" 
        border-radius="16px 16px 0 0"
        />

      <mj-class 
        name="radius-bottom" 
        border-radius="0 0 16px 16px "
        />

      <mj-class 
        name="boton-azul" 
        border-radius="50px"
        height="22px"
        width="auto"
        color="#fff"
        background-color="#2798C0"
        font-size="14px"
        align="left"
        border="2px solid #2798C0"
        />

      <mj-class 
        name="boton-blanco-outline" 
        border-radius="50px"
        height="22px"
        color="#fff"
        font-size="14px"
        background-color="transparent"
        align="left"
        border="2px solid #fff"
        />

      <mj-class 
        name="margin-resp" 
        margin="0 15px"
        />

      <mj-class 
        name="pad-40" 
        padding="30px 10px"
        />

      <mj-class 
        name="space" 
        padding-top="30px"
        />
    </mj-attributes>

    <mj-style inline="inline">
      .capitalize {
        text-transform: capitalize;
      }
      table {
        overflow: hidden;
      }
      .share-email a { display: inline-block !important; }
    </mj-style>




  </mj-head>

<mj-body background-color="#fff" width="640px">


<mj-raw>

<div id="preview_text" style="display:none;font-size:1px;color:{{background_color}};line-height:1px;max-height:0px;max-width:0px;opacity:0;overflow:hidden;">{% text "preview_text" label="Preview Text", value="", no_wrapper=True %}</div>


{% text "b1_titulo", label="Noticia 1, Título", value="Aquí pondremos el título del post, que puede ser largo en algunas ocasiones",
